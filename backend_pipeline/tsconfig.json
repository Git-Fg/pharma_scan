{
  "compilerOptions": {
    "lib": ["ESNext"],
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "bundler",
    "moduleDetection": "force",
    "allowImportingTsExtensions": true,
    "noEmit": true,                
    "strict": true,
    "noImplicitOverride": true,
    "skipLibCheck": true,
    "types": ["bun"]               
  }
}